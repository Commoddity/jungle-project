<tr>
  <td>
    <h4><%= category.name %></h4>
  </td>
  <td>
    <%= render 'admin/dashboard/link', item: category.name, url: category_path(category) %>
  </td>
  <td>
    <%= link_to fa_icon('trash'), [:admin, category], class: 'btn btn-sm btn-danger', method: :delete, data: { confirm: "Are you sure?" } %>
  </td>
</tr>